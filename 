[32m+[m[32mnamespace webapi001.Controllers[m
[32m+[m[32m{[m
[32m+[m[32m    [ApiController][m
[32m+[m[32m    [Route("[controller]")][m
[32m+[m[32m    public class WeatherForecastController : ControllerBase[m
[32m+[m[32m    {[m
[32m+[m[32m        private static readonly string[] Summaries = new[][m
[32m+[m[32m        {[m
[32m+[m[32m            "Freezing", "Bracing", "Chilly", "Cool", "Mild", "Warm", "Balmy", "Hot", "Sweltering", "Scorching"[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        private readonly ILogger<WeatherForecastController> _logger;[m
[32m+[m
[32m+[m[32m        public WeatherForecastController(ILogger<WeatherForecastController> logger)[m
[32m+[m[32m        {[m
[32m+[m[32m            _logger = logger;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        [HttpGet][m
[32m+[m[32m        public IEnumerable<WeatherForecast> Get()[m
[32m+[m[32m        {[m
[32m+[m[32m            var rng = new Random();[m
[32m+[m[32m            return Enumerable.Range(1, 5).Select(index => new WeatherForecast[m
[32m+[m[32m            {[m
[32m+[m[32m                Date = DateTime.Now.AddDays(index),[m
[32m+[m[32m                TemperatureC = rng.Next(-20, 55),[m
[32m+[m[32m                Summary = Summaries[rng.Next(Summaries.Length)][m
[32m+[m[32m            })[m
[32m+[m[32m            .ToArray();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
